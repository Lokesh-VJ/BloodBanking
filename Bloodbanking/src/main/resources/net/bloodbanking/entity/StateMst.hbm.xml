<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
	<class 
	    name="net.bloodbanking.entity.StateMst" 
	    table="state_mst"
	>
	
	    <id
	        name="stateId"
	        type="java.lang.Long"
	        column="state_id"
	    >
	        <generator class="native" />
	    </id>
	
	    <property
	        name="stateName"
	        type="java.lang.String"
	        column="state_name"
	        not-null="true"
	        length="50"
	    />
	</class>
</hibernate-mapping>

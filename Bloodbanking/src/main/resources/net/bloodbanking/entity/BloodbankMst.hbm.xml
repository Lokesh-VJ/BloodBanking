<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

	<class 
	    name="net.bloodbanking.entity.BloodbankMst" 
	    table="bloodbank_mst"
	>
	
	    <id
	        name="bID"
	        type="java.lang.Long"
	        column="b_id"
	    >
	        <generator class="native" />
	    </id>
	
	<property
	        name="regId"
	        type="java.lang.Long"
	        column="reg_id"
	        not-null="true"
	        length="4"
	    />
	   
	    <property
	        name="bbName"
	        type="java.lang.String"
	        column="bb_name"
	        not-null="true"
	        length="10"
	    />
	    
	     <property
	        name="locId"
	        type="java.lang.long"
	        column="loc_id"
	        not-null="true"
	        length="4"
	    />
	    
	     <property
	        name="contactNo"
	        type="java.lang.String"
	        column="contact_no"
	        not-null="true"
	        length="12"
	    />
	    
	     <property
	        name="status"
	        type="java.lang.long"
	        column="status"
	        not-null="true"
	        length="1"
	    />
	    
	     <property
	        name="ddate"
	        type="java.sql.Timestamp"
	        column="ddate"
	        not-null="true"
	        length="15"
	    />
	</class>
</hibernate-mapping>

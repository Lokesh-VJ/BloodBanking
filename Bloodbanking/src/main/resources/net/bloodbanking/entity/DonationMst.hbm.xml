<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

	<class 
	    name="com.nhance.entity.DonationMst" 
	    table="donation_mst"
	>
	
	    <id
	        name="dID"
	        type="java.lang.Long"
	        column="d_id"
	    >
	        <generator class="native" />
	    </id>
	
	<property
	        name="regId"
	        type="java.lang.Long"
	        column="reg_id"
	        not-null="true"
	        length="10"
	    />
	   
	    <property
	        name="name"
	        type="java.lang.String"
	        column="name"
	        not-null="true"
	        length="10"
	    />
	    
	     <property
	        name="bId"
	        type="java.lang.Long"
	        column="b_id"
	        not-null="true"
	        length="4"
	    />
	    
	     <property
	        name="bgroup"
	        type="java.lang.String"
	        column="bgroup"
	        not-null="true"
	        length="10"
	    />
	    
	     <property
	        name="ddate"
	        type="java.sql.Timestamp"
	        column="ddate"
	        not-null="true"
	        length="15"
	    />
	    
	     <property
	        name="locationId"
	        type="java.lang.Long"
	        column="location_id"
	        not-null="true"
	        length="4"
	    />
	</class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

	<class 
	    name="net.bloodbanking.entity.UsertypeMst" 
	    table="usertype_mst"
	>
	
	    <id
	        name="uId"
	        type="java.lang.Long"
	        column="u_id"
	    >
	        <generator class="native" />
	    </id>
	
	    <property
	        name="uType"
	        type="java.lang.String"
	        column="u_type"
	        not-null="true"
	        length="50"
	    />
	
	</class>
</hibernate-mapping>
